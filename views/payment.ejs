    <h3 style="margin-left: 500px;">Welcome to Payment Sneaker</h3>
    <form action="/payment" method="POST" style="margin-left: 650px;">
      <input type="hidden" name="phone" value="<%= phone %>">
      <input type="hidden" name="address" value="<%= address %>">
      <input type="hidden" name="price" value="<%= price %>">
    <script
        src="//checkout.stripe.com/v2/checkout.js"
        class="stripe-button"
        data-key="<%= key %>"
        data-amount=<%= price %>
        data-currency="usd"
        data-name="Payment with card"
        data-description="Book Empire purchase"
        data-locale="auto" >
        </script>
    </form>
    
